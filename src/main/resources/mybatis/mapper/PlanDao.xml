<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.himedia.tp01.dao.IPlanDao">
    <insert id="setPlan">
        INSERT INTO plan(code, plan_startdate, plan_enddate) VALUSE(#{param1}, #{param2}, #{param3})
    </insert>

    <select id="getPlan" resultType="com.himedia.tp01.dto.PlanVO">
        SELECT * FROM plan WHERE code=#{param1}
    </select>
</mapper>